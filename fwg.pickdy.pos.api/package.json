{
  "name": "fwg.pickdy.pos.api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/pickdy.pos.com",
    "build-ts": "tsc --project tsconfig.json -w -p .",
    "build": "tsc -p . && npm run copyfiles",
    "copyfiles": "copyfiles ./docs/*.yaml  dist/build/fwg.pickdy.pos.api",
    "build:noWatch": "tsc -p .",
    "docs:all": "npm run docs:typedoc && npm run docs:swagger",
    "docs:typedoc": "typedoc --out ./docs/typedoc/ app.ts",
    "docs:swagger": "apidoc-swagger -i ./ -o ./docs/docs_swagger/",
    "test": "echo \"Mock: All tests passed(no tests for now)\" && exit 0"
  },
  "main": "app.js",
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bufferutil": "^4.0.1",
    "cookie-parser": "~1.4.4",
    "copyfiles": "^2.1.0",
    "cors": "^2.8.5",
    "cryptlib": "^1.0.3",
    "debug": "~4.1.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.0",
    "http-errors": "~1.7.3",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "log4js": "^4.4.0",
    "mongo-querystring": "4.1.1",
    "mongodb": "3.2.7",
    "mongodb-extended-json": "1.10.1",
    "morgan": "~1.9.1",
    "path": "^0.12.7",
    "pug": "2.0.4",
    "qrcode": "^1.4.4",
    "query-string": "^6.8.1",
