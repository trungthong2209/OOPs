    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "stripe": "^7.4.0",
    "swagger-jsdoc": "^3.0.3",
    "swagger-ui-express": "^4.0.1",
    "typescript": "^3.5.2",
    "utf-8-validate": "^5.0.2",
    "ws": "7.3.0"
